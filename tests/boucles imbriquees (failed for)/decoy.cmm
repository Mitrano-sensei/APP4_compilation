int main() {
    
    int i, j, res;
    res = 0;

    i = 0;
    j = 0;

    while(i < 10)
    {
        i = i+1;
        j = 0;
        while (j < 10)
        {
            j = j + 1;
        }
    }
    
    print((i == 10) && (j==10));

    do
    {
        j = 0;
        i = i + 1;
        do {
            j = j+1;
        } while (j < 10);
    } while (i < 10);

    print((i==11) && (j==10));

    res = 0;

	for(i = 0; i < 10; i = i + 1 )
	{
		for (j = 0; j < 10; j = j + 1)
		{
			res = res + 1;
		}
	}

    return (res == 100);
}
