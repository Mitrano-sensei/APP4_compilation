int main() {
	int a;

	for (a = 0; a < 10; a = a + 1)
	{
		if (a == 5)
		{
			break;
		}
	}

	int b;
	b = 0;

	while (b < 10)
	{
		b = b + 1;
		if (b == 5)
		{
			break;
		}
	}

	int c;
	c = 0;

	do
	{
		c = c + 1;
		if (c == 5)
		{
			break;
		}
	} while (c < 10);
	
	return (a == 5) && (b == 5) && (c == 5);
}